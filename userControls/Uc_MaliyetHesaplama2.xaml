<UserControl x:Class="MaliyeHesaplama.userControls.Uc_MaliyetHesaplama2"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:MaliyeHesaplama.mvvm"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <UserControl.DataContext>
        <local:MainViewModel />
    </UserControl.DataContext>
    <StackPanel>
        <TextBox Text="{Binding Atki1IpBil1, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Width="100"></TextBox>
        <TextBox Text="{Binding Atki1IpBil2, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Width="100"></TextBox>
        <TextBox Text="{Binding Atki1IpBil3, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Width="100"></TextBox>
    </StackPanel>
</UserControl>
