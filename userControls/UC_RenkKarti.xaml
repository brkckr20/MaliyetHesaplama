<UserControl x:Class="MaliyeHesaplama.userControls.UC_RenkKarti"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:MaliyeHesaplama.userControls"
             xmlns:common="clr-namespace:MaliyeHesaplama.userControls.Common"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <common:ButtonBar x:Name="ButtonBar" DockPanel.Dock="Top" Grid.Row="0"/>
        <StackPanel Grid.Row="1">
            <DockPanel>
                <GroupBox Header="Renk Türü  - BU EKRAN DEVAM EDİLECEK ">
                    
                </GroupBox>
                <CheckBox Content="Kullanımda"/>
            </DockPanel>
        </StackPanel>
    </Grid>
</UserControl>
