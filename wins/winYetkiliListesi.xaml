<Window x:Class="MaliyeHesaplama.wins.winYetkiliListesi"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MaliyeHesaplama.wins"
        mc:Ignorable="d"
        xmlns:control="clr-namespace:FilterDataGrid;assembly=FilterDataGrid"
        Title="Yetkilisi Listesi" FontFamily="Bahnschrift" Height="450" Width="450" Icon="/wins/News.png" WindowStartupLocation="CenterScreen" ResizeMode="NoResize" Loaded="Window_Loaded">
    <Grid>
        <control:FilterDataGrid
    x:Name="mygrid" FontSize="12" VerticalContentAlignment="Center" IsReadOnly="True"
    GridLinesVisibility="All" HorizontalGridLinesBrush="#ececec" VerticalGridLinesBrush="#ececec"
    FilterLanguage="Turkish" ShowStatusBar="True" AutoGenerateColumns="True"
    AutoGeneratingColumn="mygrid_AutoGeneratingColumn" MouseDoubleClick="mygrid_MouseDoubleClick"
    />
    </Grid>
</Window>
