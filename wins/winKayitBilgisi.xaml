<Window x:Class="MaliyeHesaplama.wins.winKayitBilgisi"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MaliyeHesaplama.wins"
        mc:Ignorable="d"
        Title="Kayıt Bilgisi" Height="150" 
        Width="382" WindowStartupLocation="CenterScreen" FontFamily="Bahnschrift Light" 
        Icon="/wins/News.png"
        Loaded="Window_Loaded" ResizeMode="NoResize">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <GroupBox Header="Kayıt Eden : " Grid.Column="0">
            <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
                <Label x:Name="lbInsertUser" HorizontalContentAlignment="Center"/>
                <Label x:Name="lbInsertDate" HorizontalContentAlignment="Center"/>
                <Label x:Name="lbInsertTime" HorizontalContentAlignment="Center"/>
            </StackPanel>
        </GroupBox>
        <GroupBox Header="Güncelleyen : " Grid.Column="1">
            <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
                <Label x:Name="lbUpdateUser" HorizontalContentAlignment="Center"/>
                <Label x:Name="lbUpdateDate" HorizontalContentAlignment="Center"/>
                <Label x:Name="lbUpdateTime" HorizontalContentAlignment="Center"/>
            </StackPanel>
        </GroupBox>
    </Grid>
</Window>
